    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.stomhong.ui.activity.WebViewActivity"
    android:orientation="vertical">

    <TextView
        android:id="@+id/id_tv_webview_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="标题"/>

    <WebView
        android:id="@+id/id_wv_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </WebView>
</LinearLayout>
